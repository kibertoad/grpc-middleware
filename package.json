{
  "name": "grpc-middleware",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kibertoad/grpc-middleware.git"
  },
  "keywords": [
    "grpc",
    "middleware"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Igor Savin",
  "license": "MIT",
  "scripts": {
    "test": "mocha test/**/*.spec.js",
    "test-with-coverage": "nyc mocha --opts \"./mocha.opts\" test",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "eslint": "eslint --format codeframe \"lib/**/*.js\" \"test/**/*.js\""
  },
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "protobuf-fieldmask": "^1.0.1",
    "lodash.get": "^4.4.2",
    "uuid": "^3.3.2",
    "validation-utils": "^2.8.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "grpc": "^1.16.1",
    "grpc-caller": "^0.11.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2"
  },
  "nyc": {
    "description": "test coverage",
    "exclude": [
      "node_modules",
      "test",
      "build"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/reports/coverage",
    "check-coverage": true,
    "lines": 85,
    "statements": 85,
    "functions": 90,
    "branches": 85
  }
}
